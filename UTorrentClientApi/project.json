{
  "version": "1.0.1-*",
  "authors": [ "latop2604" ],
  "title": "UTorrentClient Api",
  "description": "UTorrentClient Api is an extensible set of classes that use WebUI to manipulate µTorrent remotely.",
  "packOptions": {
    "summary": "UTorrentClient Api is an extensible set of classes that use WebUI to manipulate µTorrent remotely.",
    "tags": [ "µTorrent", "uTorrent", "Torrent" ],
    "owners": [ "latop2604" ],
    "projectUrl": "https://github.com/latop2604/UTorrentClientApi",
    "releaseNotes": "https://github.com/latop2604/UTorrentClientApi",
    "licenseUrl": "https://utorrentclientapi.codeplex.com/license",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/latop2604/UTorrentClientApi"
    }
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "UTorrent.Api.Data": "1.0.1-*",
    "Newtonsoft.Json": "9.0.1"
  },

  "frameworks": {
    "netstandard1.3": {
      "buildOptions": {
        "define": [
          "PORTABLE"
        ]
      },
      "imports": "dnxcore50",
      "dependencies": {
        "System.Diagnostics.Contracts": "4.0.1",
        "System.Net.Requests": "4.0.11",
        "System.Runtime.Extensions": "4.1.0",
        "System.Net.Primitives": "4.0.11"
      }
    },
    "NET46": {
      "frameworkAssemblies": {
        "System.Configuration": ""
      }
    },
    "NET45": {
      "frameworkAssemblies": {
        "System.Configuration": ""
      }
    }
  }
}
